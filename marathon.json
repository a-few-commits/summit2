{
  "id": "/summit2",
  "cpus": 1,
  "mem": 128,
  "instances": 1,
  "cmd": "shiny-server",
  "container": {
    "docker": {
      "image": "registry.marathon.l4lb.thisdcos.directory:5000/summit2:latest",
      "portMappings": [
        {
          "containerPort": 3838,
          "protocol": "tcp",
          "name": "http"
        }
      ],
      "network": "BRIDGE"
    }
  },
  "labels": {
    "DCOS_SERVICE_PORT_INDEX": "0",
    "DCOS_SERVICE_SCHEME": "http",
    "DCOS_SERVICE_NAME": "summit2"
  }
}
